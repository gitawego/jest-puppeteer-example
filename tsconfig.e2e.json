{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": true,
		"outDir": "build",
		"lib": ["es6", "dom", "esnext"],
		"types": ["jest"],
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}
